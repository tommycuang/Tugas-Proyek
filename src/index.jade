	section
		h1 String
		ul
			li
			li Bentuk data yang biasa dipakai dalam bahasa pemrograman untuk keperluan menampung dan memanipulasi data teks, misalnya untuk menampung (menyimpan) suatu kalimat atau karakter.
			br
			li Merupakan kumpulan dari array dengan tipe data char.
			br
			li Sederetan simbol yang tak tertentu panjangnya, yang dianggap sebagai panjang satu unit
			br
			li Berupa huruf, angka, karakter khusus, maupun karakter Unicode
			br
			li Terdapat beberapa fungsi yang digunakan untuk operasi pengolahan string

	section
		h2 Fungsi String pada C++ Style
		ul
			li
			li o Fungsi size / length
				ul
					li Mengembalikan panjang karakter suatu string
			li o Fungsi insert
				ul
					li Menyisipkan karakter-karakter ke dalam string
			li o Fungsi erase
				ul
					li Menghapus karakter tertentu pada string
			li o Fungsi clear
				ul
					li Menghapus string
			li o Fungsi substr
				ul
					li Mengenerate substing dari string
			li o Fungsi operator+
				ul
					li Mengabungkan string-string
			li o Fungsi getline
				ul
					li Membaca baris dari stream ke dalam string

